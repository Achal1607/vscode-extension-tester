{
	"name": "test-project",
	"displayName": "test-project",
	"description": "Test Project for VS Code Extension Tester",
	"version": "0.0.1",
	"publisher": "vscode-extension-tester",
	"repository": "https://github.com/jrichter1/vscode-extension-tester/test/test-project/",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.openFile",
		"onCommand:extension.openFolder",
		"onCommand:extension.closeFolder"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.openFile",
				"title": "Open Test File"
			},
			{
				"command": "extension.openFolder",
				"title": "Open Test Folder"
			},
			{
				"command": "extension.closeFolder",
				"title": "Close Test Folder"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./",
		"ui-test": "npm run compile && extest setup-and-run './out/test/**/*test.js'"
	},
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/node": "^8.10.25",
		"@types/vscode": "^1.34.0",
		"chai": "^4.2.0",
		"mocha": "^6.2.0",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"vscode-extension-tester": "file:../.."
	}
}
